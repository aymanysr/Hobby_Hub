  <%= month_calendar(events: meetings, end_attribute: :end_time) do |date, meetings| %>
    <div class="day">
      <%= date %>
    </div>
    <% meetings.each do |meeting| %>
      <div class="meeting">
        <div class="card-title">
          <h1><%= meeting.name %></h1>
          <small><%= meeting.time unless meeting.multi_days? %></small>
        </div>
      </div>
      <div class="card-body">
        <h3 class="card-text">
          <%= meeting.description %>
        </h3>
      </div>

      <div class="card-footer">
        <p class="card-text">
        On: <%= meeting.start_time.strftime('%A, %B %d, %Y at %I:%M %p') %>
        </p>
      </div>
    <% end %>
  <% end %>
